			-- Position the attachment using CFrame
			
            
			zzz1.Parent = plr.Character
			zzz1.AlignOrientation.Attachment1 = plr.Character:WaitForChild("HumanoidRootPart").Book
			zzz1.AlignPosition.Attachment1 = plr.Character.HumanoidRootPart.Book
			wait(5)
			attachment.Orientation = Vector3.new(68.709, -133.693, 151.346)
			local TweenService = game:GetService("TweenService")
			local function createFloatingMotion(attachment)
				-- Define the start and end positions for the float
				local startPosition = attachment.Position
				local endPosition = startPosition + Vector3.new(0, 1, 0) -- Move up by 1 unit

				-- TweenInfo for smooth motion
				local tweenInfo = TweenInfo.new(
					1, -- Duration (1 second for up or down)
					Enum.EasingStyle.Sine, -- Smooth easing style
					Enum.EasingDirection.InOut, -- Smooth transition in and out
					-1, -- Repeat indefinitely
					true -- Reverse after each tween (creates up-and-down motion)
				)

				-- Tween the Position property of the attachment
				local tween = TweenService:Create(
					attachment,
					tweenInfo,
					{ Position = endPosition }
				)

				tween:Play()
			end

			-- Start floating motion for the attachment
			createFloatingMotion(attachment)
			
		end)
	end
